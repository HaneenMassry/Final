<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#009C27B0"
    android:backgroundTint="#004CAF50"
    android:rotationX="360"
    android:scrollbarTrackVertical="@drawable/blackboard"
    tools:context="afinal.example.afinal.ActivityStart$ActivityStart">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="125dp"
        android:layout_height="87dp"
        android:background="#002196F3"
        android:src="@drawable/blackboard"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.041"
        app:srcCompat="@drawable/logo_easy_learn" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="125dp"
        android:layout_height="87dp"
        android:background="#002196F3"
        android:src="@drawable/blackboard"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.944"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.989"
        app:srcCompat="@drawable/logo_easy_learn" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="471dp"
        android:layout_height="560dp"
        android:layout_marginBottom="132dp"
        android:background="#008BC34A"
        android:src="@drawable/blackboard"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="-44dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="181dp"
        android:layout_height="93dp"
        android:text="Welcome"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="115dp"
        tools:layout_editor_absoluteY="232dp" />

</androidx.constraintlayout.widget.ConstraintLayout>